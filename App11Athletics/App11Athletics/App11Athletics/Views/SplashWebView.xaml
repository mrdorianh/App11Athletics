<?xml version="1.0" encoding="utf-8" ?>
<ContentPage x:Class="App11Athletics.Views.SplashWebView"
             xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:me="clr-namespace:App11Athletics.MarkupExtensions;assembly=App11Athletics" xmlns:o="clr-namespace:Octane.Xam.VideoPlayer;assembly=Octane.Xam.VideoPlayer"
             BackgroundColor="White">
    <Grid>
        <o:VideoPlayer x:Name="videoPlayer"
                       AutoPlay="False" Completed="VideoPlayer_OnCompleted" DisplayControls="False" Failed="VideoPlayer_OnFailed" FillMode="ResizeAspectFill" Playing="VideoPlayer_OnPlaying"
                       Source="{me:VimeoVideoId 206658139}"
                       BackgroundColor="White" />
        <BoxView x:Name="boxView"
                 Margin="0"
                 HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand"
                 Color="White" BackgroundColor="White" />
    </Grid>
</ContentPage>